{
  "name": "web-speed-hackathon-online",
  "version": "0.0.1",
  "license": "Proprietary",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all clean -p build:css build:backend build:webpack",
    "build:backend": "babel --config-file ./.backend.babelrc --out-dir dist/backend src/backend",
    "build:css": "postcss src/client/app.css -d dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:webpack-with-mock-data": "cross-env USE_MOCK_DATA=true NODE_ENV=production webpack --config webpack.config.js",
    "fmt": "prettier --write 'src/**/*.{js,css}'",
    "serve": "nodemon dist/backend/server.js"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,css}": "prettier --write"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@zendeskgarden/css-buttons": "^7.0.19",
    "@zendeskgarden/css-forms": "^7.0.20",
    "@zendeskgarden/css-utilities": "^4.5.5",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.29",
    "express": "^4.17.1",
    "immutable": "^4.0.0-rc.12",
    "lodash.chunk": "^4.2.0",
    "lodash.shuffle": "^4.2.0",
    "normalize.css": "^8.0.1",
    "preact": "^10.4.6",
    "race-timeout": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redaxios": "^0.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.5",
    "suitcss-utils": "^3.0.0"
  }
}
